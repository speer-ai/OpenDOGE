# FPDS API settings
FPDS_API_URL=https://api.fpds.gov/api/v1
FPDS_API_KEY=your_fpds_api_key

# FBO API settings (SAM.gov Contract Opportunities)
FBO_API_URL=https://api.sam.gov/opportunities/v1
FBO_API_KEY=your_fbo_api_key

# FSRS API settings
FSRS_API_URL=https://api.fsrs.gov/api/v1
FSRS_API_KEY=your_fsrs_api_key

# FRED API settings
FRED_API_URL=https://api.stlouisfed.org/fred/v1
FRED_API_KEY=your_fred_api_key

# SEC EDGAR API settings
SEC_EDGAR_API_URL=https://data.sec.gov/api
SEC_USER_AGENT=OpenDOGE/1.0.0 (your@email.com)

